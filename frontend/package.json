"lodash.get": "^4.4.2",
"lodash.merge": "^4.6.2",
"moment-timezone": "^0.5.45",
"password-validator": "^5.3.0",
"pinia": "^2.3.0",
"quill": "^1.3.7",
"quill-delta": "^5.1.0",
"quill-delta-to-markdown": "^0.6.0",
"swiper": "^11.2.4",
