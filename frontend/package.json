{
"name": "rncp-front",
"version": "0.1.0",
"private": true,
"scripts": {
"dev": "node --max-old-space-size=8192 ./node_modules/vite/bin/vite.js",
"build": "run-p type-check build-only",
"preview": "vite preview",
"build-only": "vite build",
"staging": "run-p type-check build-staging",
"build-staging": "vite build --mode staging",
"type-check": "vue-tsc --noEmit",
"build-prod": "vite build --mode production",
"start": "vite preview --port 41100 --host",
"prod": "npm run build-prod && pm2 start npm --name RHCN-Front -- run start"
},
"dependencies": {
